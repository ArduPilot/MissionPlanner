    <PackageReference Include="MP.GDAL.Native" Version="2.2.3" />
    <PackageReference Include="System.Drawing.Common" Version="4.5.1" />
    <ProjectReference Include="..\GMap.NET.Core\GMap.NET.Core.csproj" />
	
  </ItemGroup>

