<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
  <appSettings>
    <add key="UpdateLocationVersion" value="http://firmware.diydrones.com/MissionPlanner/upgrade/" />
    <add key="UpdateLocation" value="http://firmware.diydrones.com/MissionPlanner/upgrade/" />
    <add key="UpdateLocationMD5" value="http://firmware.diydrones.com/MissionPlanner/checksums.txt" />
    <add key="BetaUpdateLocationVersion" value="http://oborne.me/MissionPlanner/upgrade/" />
    <add key="BetaUpdateLocation" value="http://oborne.me/MissionPlanner/upgrade/" />
    <add key="BetaUpdateLocationMD5" value="http://oborne.me/MissionPlanner/checksums.txt" />
    <add key="ParameterLocations"
         value="https://raw.githubusercontent.com/diydrones/ardupilot/ArduCopter-stable/ArduCopter/Parameters.pde;
         https://raw.githubusercontent.com/diydrones/ardupilot/ArduCopter-stable/ArduCopter/Parameters.cpp;
         https://raw.githubusercontent.com/diydrones/ardupilot/ArduPlane-stable/ArduPlane/Parameters.pde;
         https://raw.githubusercontent.com/diydrones/ardupilot/ArduPlane-stable/ArduPlane/Parameters.cpp;
         https://raw.githubusercontent.com/diydrones/ardupilot/APMrover2-stable/APMrover2/Parameters.pde;
         https://raw.githubusercontent.com/diydrones/ardupilot/APMrover2-stable/APMrover2/Parameters.cpp;
         https://raw.githubusercontent.com/diydrones/ardupilot/master/AntennaTracker/Parameters.pde;
         https://raw.githubusercontent.com/diydrones/ardupilot/master/AntennaTracker/Parameters.cpp;" />
    <add key="ParameterLocationsBleeding"
         value="https://raw.githubusercontent.com/diydrones/ardupilot/master/ArduCopter/Parameters.pde;
         https://raw.githubusercontent.com/diydrones/ardupilot/master/ArduCopter/Parameters.cpp;
         https://raw.githubusercontent.com/diydrones/ardupilot/master/ArduPlane/Parameters.cpp;
         https://raw.githubusercontent.com/diydrones/ardupilot/master/APMrover2/Parameters.cpp;
         https://raw.githubusercontent.com/diydrones/ardupilot/master/AntennaTracker/Parameters.pde;
         https://raw.githubusercontent.com/diydrones/ardupilot/master/AntennaTracker/Parameters.cpp;" />
    <add key="ParameterMetaDataXMLFileName" value="ParameterMetaData.xml" />
    <add key="ParameterMetaDataXMLFileNameBackup" value="ParameterMetaDataBackup.xml" />
  </appSettings>
  <log4net>
    <appender name="Console" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level %logger - %message%newline" />
      </layout>
      <threshold value="INFO" />
    </appender>
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <file value="MissionPlanner.log" />
      <appendToFile value="true" />
      <maximumFileSize value="500KB" />
      <maxSizeRollBackups value="2" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %5level %logger - %message (%file:%line) [%thread]%newline" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="RollingFile" />
      <appender-ref ref="Console" />
    </root>
  </log4net>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Runtime" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.5.19.0" newVersion="2.5.19.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Threading.Tasks" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.5.19.0" newVersion="2.5.19.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Net.Http.Primitives" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.1.10.0" newVersion="2.1.10.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Data.SQLite" publicKeyToken="db937bc2d44ff139" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.0.84.0" newVersion="1.0.84.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>